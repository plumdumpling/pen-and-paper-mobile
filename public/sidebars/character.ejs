<div id="character" data-requested="false">
  <div class="top">
    <h1>Charakterbogen</h1>

    <div id="editCharacter"></div>

    <div class="infoText">
      <h3><%= response.informations.name %> (<%= response.informations.age %>)</h3>
      <p><%= response.informations.occupation %> aus <%= response.informations.origin %></p><br/>
      <p><%= response.informations.description %></p>
    </div>
  </div>
  <!-- dotted line -->
  <svg width="100%" height="6px" class="dottedLine">
    <line x1="0" x2="2000" y1="2" y2="2" stroke="#fff" stroke-width="3" stroke-linecap="round" stroke-dasharray="6, 8"/>
  </svg>
  <div class="bottom">
    <div id="characterFormOuter">
      <div id="characterFormInner">
        <% include ../manageContent/characterForm %>
      </div>
    </div>
  </div>
</div>
